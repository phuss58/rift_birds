# install EEMS

git clone https://github.com/dipetkov/eems.git

conda create -n eems

source activate eems

conda install boost

conda install eigen

cd /home/cooperlab/phuss58eems/runeems_snps/src

# change the following in the runeems_snps makefile:
EIGEN_INC = /home/cooperlab/phuss58/.conda/envs/eems/include/eigen3
BOOST_LIB = /home/cooperlab/phuss58/.conda/envs/eems/lib
BOOST_INC = /home/cooperlab/phuss58/.conda/envs/eems/include

module load gnu/4.5

make linux



